<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="450dp"
        android:layout_gravity="center_horizontal"
        android:padding="5dp"
        android:orientation="vertical" >

        <!-- First Row -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:orientation="horizontal"
            android:weightSum="2" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:background="@color/entity_breast"
                android:clickable="true"
                android:onClick="addBreastOnClick"
                android:orientation="horizontal" >

                <ImageButton
                    android:id="@+id/addBreast"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/transparent"
                    android:onClick="addBreastOnClick"
                    android:padding="10dp"
                    android:src="@drawable/icon_breast" >
                </ImageButton>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingRight="10dp" >

                    <TextView
                        android:id="@+id/breastDir"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Баруун мээм"
                        android:textColor="#fff" />

                    <TextView
                        android:id="@+id/breastTime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0ц 10м өмнө"
                        android:textColor="#fff" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:background="@color/entity_feed"
                android:clickable="true"
                android:onClick="extraFoodOnClick" >

                <ImageButton
                    android:id="@+id/extraFood"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/transparent"
                    android:clickable="true"
                    android:onClick="extraFoodOnClick"
                    android:padding="10dp"
                    android:src="@drawable/icon_extrafood" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingRight="10dp" >

                    <TextView
                        android:id="@+id/extraFoodName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Төмсний нухаш"
                        android:textColor="#fff" />

                    <TextView
                        android:id="@+id/extraFoodTime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1ц 12м өмнө"
                        android:textColor="#fff" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:orientation="horizontal"
                android:weightSum="2" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@color/entity_formula"
                    android:clickable="true"
                    android:onClick="formulaOnClick" >

                    <ImageButton
                        android:id="@+id/formula"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:onClick="formulaOnClick"
                        android:padding="10dp"
                        android:src="@drawable/icon_formula" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/formulaName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Нэмэлт сүү"
                            android:textColor="#fff" />

                        <TextView
                            android:id="@+id/formulaTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1ц 12м өмнө"
                            android:textColor="#fff" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@color/entity_drink"
                    android:clickable="true"
                    android:onClick="drinkOnClick" >

                    <ImageButton
                        android:id="@+id/drink"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:onClick="drinkOnClick"
                        android:padding="10dp"
                        android:src="@drawable/icon_drink" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/drinkName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Хярам"
                            android:textColor="#fff" />

                        <TextView
                            android:id="@+id/drinkTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1ц 12м өмнө"
                            android:textColor="#fff" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <!-- Second Row -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:orientation="horizontal"
                android:weightSum="2" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@color/entity_anniversary"
                    android:clickable="true"
                    android:onClick="hospitalOnClick" >

                    <ImageButton
                        android:id="@+id/hospital"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:onClick="hospitalOnClick"
                        android:padding="10dp"
                        android:src="@drawable/icon_gohospital" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/hospitalName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Эмчийн үзлэг"
                            android:textColor="#fff" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@color/entity_learn"
                    android:clickable="true"
                    android:onClick="medcheckOnClick" >

                    <ImageButton
                        android:id="@+id/medcheck"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:onClick="medcheckOnClick"
                        android:padding="10dp"
                        android:src="@drawable/icon_medcheck" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/medcheckName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Ээлжит үзлэг"
                            android:textColor="#fff" />

                        <TextView
                            android:id="@+id/medcheckTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1ц 12м өмнө"
                            android:textColor="#fff" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:orientation="horizontal"
                android:weightSum="2" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@color/entity_purchase"
                    android:clickable="true"
                    android:onClick="vitaminOnClick" >

                    <ImageButton
                        android:id="@+id/vitamin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:onClick="vitaminOnClick"
                        android:padding="10dp"
                        android:src="@drawable/icon_vitamin" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/vitaminName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Витамин"
                            android:textColor="#fff" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@color/entity_tooth"
                    android:clickable="true"
                    android:onClick="vaccineOnClick" >

                    <ImageButton
                        android:id="@+id/vaccine"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:onClick="vaccineOnClick"
                        android:padding="10dp"
                        android:src="@drawable/icon_vaccine" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/vaccineName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Вакцин"
                            android:textColor="#fff" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <!-- Third Row -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:orientation="horizontal"
                android:weightSum="2" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@color/entity_ao_ear"
                    android:clickable="true"
                    android:onClick="anniversaryOnClick" >

                    <ImageButton
                        android:id="@+id/learn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:onClick="anniversaryOnClick"
                        android:padding="10dp"
                        android:src="@drawable/icon_birthmonth" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/learnName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Би чадна"
                            android:textColor="#fff" />

                        <TextView
                            android:id="@+id/learnValue"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Мөлхдөг болсон"
                            android:textColor="#fff" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@color/entity_ao_sleep"
                    android:clickable="true"
                    android:onClick="toothOnClick" >

                    <ImageButton
                        android:id="@+id/tooth"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:onClick="toothOnClick"
                        android:padding="10dp"
                        android:src="@drawable/icon_teeth" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/toothName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Шинэ шүд"
                            android:textColor="#fff" />

                        <TextView
                            android:id="@+id/toothValue"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="6 шүдтэй"
                            android:textColor="#fff" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <!-- Fifth Row -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:orientation="horizontal"
                android:weightSum="2" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@color/entity_ao_shower"
                    android:clickable="true"
                    android:onClick="changeDiaperOnClick" >

                    <ImageButton
                        android:id="@+id/changediaper"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:onClick="changeDiaperOnClick"
                        android:padding="10dp"
                        android:src="@drawable/icon_diaperchange" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/changediaperName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Баастай живх"
                            android:textColor="#fff" />

                        <TextView
                            android:id="@+id/changediaperLabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="1ц 12м өмнө"
                            android:textColor="#fff" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@color/entity_changediaper"
                    android:clickable="true"
                    android:onClick="showerOnClick" >

                    <ImageButton
                        android:id="@+id/activeOperation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:background="@android:color/transparent"
                        android:clickable="true"
                        android:onClick="showerOnClick"
                        android:padding="10dp"
                        android:src="@drawable/icon_bath" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingRight="10dp" >

                        <TextView
                            android:id="@+id/activeOperationName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Усанд орсон"
                            android:textColor="#fff" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</ScrollView>